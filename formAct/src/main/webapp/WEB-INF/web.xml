<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>formAct</display-name>
  <welcome-file-list>
    <welcome-file>/view/index/index.html</welcome-file>
    <welcome-file>/view/index/index.jsp</welcome-file>
    <welcome-file>/view/index/default.html</welcome-file>
    <welcome-file>/view/index/default.jsp</welcome-file>
  </welcome-file-list>
  
  
  
    <resource-ref>
    <description>JNDI reference to a data source</description>
    <res-ref-name>jdbc/formactds</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  
  
 <env-entry>
  <env-entry-name>Services</env-entry-name>
  <env-entry-type>Service.class</env-entry-type>
  <env-entry-value>LoginServices=formAct.autenticazione.control.LoginServices/
  				   LogoutServices=formAct.autenticazione.control.LogoutServices	</env-entry-value>
   
 </env-entry>
	
		
		
  
  
</web-app>